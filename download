from pytube import Playlist
from pytube import YouTube
p = YouTube('https://www.youtube.com/watch?v=pmKyG3NBY_k&t=2s')
print(f'Downloading: {p.title}')
directory = 'C:\\Users\name_Computer\\Video'
p.streams.first().download(directory)
